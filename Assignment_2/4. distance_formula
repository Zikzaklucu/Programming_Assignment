#include <bits/stdc++.h>
using namespace std;

int main()
{
	int x1,x2,y1,y2;
	cout << "masukkan x1 " ;
	cin >> x1;
	cout << "masukkan y1 " ;
	cin >> y1;

	cout << "masukkan x2 " ;
	cin >> x2;
	cout << "masukkan y2 " ;
	cin >> y2;

	float d = sqrt((pow((x1-x2), 2)) + pow((y1-y2), 2));
	cout << d;
}
