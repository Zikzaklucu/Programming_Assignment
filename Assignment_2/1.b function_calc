#include <bits/stdc++.h>
using namespace std;

int main()
{
    cout << "masukkan nilai dari a, b, c, d (sesuai urutan) " << endl;
    int n = 4;
    float var[4];
    for (int i = 0; i < n; i++){
        cin >> var[i];                                                      
    }
   float m = ((var[0] - var[1]) / (3 * var[0] * var[2])) * (1 - (var[1] / var[3])); 
    cout << "hasil dari fungsi m = (a-b/3ac) * (1-b/d) adalah " << m << endl;
}
